from enaml.widgets.api import Action, Menu, MenuBar



enamldef Menu_View(MenuBar):
    
    attr model
    
    Menu:
        title = '&File'

        Action:
            text = '&Load Graph'
            triggered :: 
                model.load_graph()
                print 'Graph Loaded'
                
        Action:
            text = '&Save Graph'
            triggered :: 
                model.save_graph()
                print 'Graph Saved'


