# Enaml
from enaml.widgets.api import Form, GroupBox

# Models
from calculations._core.ABCs import ABCDataFrameToDataFrame



class DataFrameIsNull_Model(ABCDataFrameToDataFrame):


    calc_name = 'Is Null'


    def setInputs(self, dataframe):

        self._dataframe = dataframe      


    def getOutputs(self):

        try:
            
            return {'dataframe': self._dataframe.isnull()}        

        except Exception as e:
            
            return self.standard_exception(e)   



enamldef DataFrameIsNull_View(GroupBox):

    Form:
        
        pass
